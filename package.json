{
  "name": "azion-platform-kit",
  "version": "1.17.0",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/aziontech/azion-platform-kit.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "ci": "yarn install --frozen-lockfile",
    "test:unit": "TZ=UTC vitest --ui",
    "test:unit:headless": "TZ=UTC vitest",
    "test:unit:coverage": "TZ=UTC vitest run --coverage",
    "test:e2e:run:dev": "start-server-and-test 'vite dev --port 5173' http://localhost:5173 'cypress run --e2e'",
    "test:e2e:run:preview-dev": "start-server-and-test preview http://localhost:5173 'cypress run --e2e'",
    "test:e2e:run:prod": "cypress run --e2e --config-file cypress.config.prod.js --env environment=prod",
    "test:e2e:run:preview-prod": "cypress run --e2e --config-file cypress.config.preview-prod.js --env environment=preview-prod",
    "test:e2e:open:dev": "start-server-and-test 'vite dev --port 5173' http://localhost:5173 'cypress open --e2e'",
    "test:e2e:open:preview-dev": "start-server-and-test preview http://localhost:5173 'cypress open --e2e'",
    "test:e2e:open:prod": "cypress open --e2e --config-file cypress.config.prod.js --env environment=prod",
    "test:e2e:open:preview-prod": "cypress open --e2e --config-file cypress.config.preview-prod.js --env environment=preview-prod",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "husky install"
  },
  "dependencies": {
    "@analytics/segment": "^2.1.0",
    "@cypress/code-coverage": "^3.12.39",
    "@feedback-fish/vue": "^1.0.1",
    "@guolao/vue-monaco-editor": "^1.3.0",
    "@stripe/stripe-js": "^4.1.0",
    "@vee-validate/yup": "^4.11.1",
    "@vueuse/core": "^10.7.0",
    "analytics": "^0.8.11",
    "axios": "^1.6.2",
    "c3": "^0.7.20",
    "cypress-real-events": "^1.13.0",
    "front-matter": "^4.0.2",
    "lottie-web-vue": "^2.0.7",
    "nyc": "^15.1.0",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.2.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.35.0",
    "primevue-sass-theme": "git+https://github.com/primefaces/primevue-sass-theme.git#3.39.0",
    "qrcode.vue": "^3.4.1",
    "recaptcha-v3": "^1.10.0",
    "vee-validate": "^4.11.1",
    "vite-plugin-istanbul": "^6.0.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/c3": "^0.7.11",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-v8": "^0.34.5",
    "@vitest/ui": "^0.34.5",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.14",
    "caniuse-lite": "^1.0.30001584",
    "cypress": "13.9.0",
    "eslint": "^8.45.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "sass": "^1.68.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "vite": "^4.5.3",
    "vitest": "^0.33.0",
    "vitest-sonar-reporter": "^1.0.0"
  }
}
